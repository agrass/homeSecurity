<h1>Listing ip_configs</h1>

<table>
  <tr>
    <th>Ip</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @ip_configs.each do |ip_config| %>
  <tr>
    <td><%= ip_config.ip %></td>
    <td><%= link_to 'Show', ip_config %></td>
    <td><%= link_to 'Edit', edit_ip_config_path(ip_config) %></td>
    <td><%= link_to 'Destroy', ip_config, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

